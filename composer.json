{
    "name": "mocode/sanctum-mongodb",
    "description": "Laravel Sanctum Mongodb provides a featherweight authentication system for SPAs and simple APIs.",
    "keywords": ["laravel", "sanctum", "mongodb", "auth"],
    "license": "MIT",
    "authors": [
        {
            "name": "mocode",
            "email": "mocode@163.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "illuminate/contracts": "^10.0|^11.0",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "mongodb/mongodb": "dev-master"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Mocode\\Sanctum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mocode\\Sanctum\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Mocode\\Sanctum\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
